generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  content   String?
  slug      String @unique
  tag       String
  createdAt DateTime?
}